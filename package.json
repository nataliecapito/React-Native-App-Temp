{
  "name": "React-Native-App-Temp",
  "version": "1.0.0",
  "description": "React Native - native application blank start template.",
  "repository": "https://github.com/smacknat/React-Native-App-Temp.git",
  "author": "smacknat",
  "license": "ISC",
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "lint": "eslint content/javascript/*",
    "lint:airlint": "eslint -c .eslintrc.airbnb content/javascript/*",
    "lint:pretty": "prettier --semi false --print-width 100 --single-quote --trailing-comma all --write \"content/javascript/**/*.js\"",
    "test": "yarn lint",
    "mobile:ios": "react-native run-ios --simulator='iPhone X'",
    "mobile:ios:8normal": "react-native run-ios --simulator='iPhone 8'",
    "mobile:ios:8plus": "react-native run-ios --simulator='iPhone 8 Plus'",
    "mobile:ios:air": "react-native run-ios --simulator='iPad Air'",
    "mobile:ios:build": "react-native bundle --entry-file='index.js' --bundle-output='./ios/App/main.jsbundle' --dev=false --platform='ios' --assets-dest='./ios'",
    "mobile:ios:build:release": "react-native run-ios --configuration Release",
    "mobile:android": "react-native run-android",
    "mobile:android:run:emulator": "/Users/$USER/Library/Android/sdk/tools/emulator -avd react-native-larger-device -netdelay none -netspeed full",
    "mobile:android:run:emulator:darin": "/Users/$USER/Library/Android/sdk/tools/emulator -avd react-native-darin-device -netdelay none -netspeed full",
    "mobile:android:run:emulator:tablet": "/Users/$USER/Library/Android/sdk/tools/emulator -avd react-native-tablet-device -netdelay none -netspeed full",
    "mobile:android:clean": "cd ./android && ./gradlew clean",
    "mobile:android:clean:trace": "cd ./android && ./gradlew --info clean build",
    "mobile:android:app:dependencies": "cd ./android && ./gradlew :app:dependencies",
    "mobile:android:build": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res"
  },
  "dependencies": {
    "@ptomasroos/react-native-multi-slider": "0.0.14",
    "@sentry/wizard": "^0.10.2",
    "barcode-reader": "^1.6.1",
    "es6-symbol": "^3.1.1",
    "events": "^3.0.0",
    "firebase": "^5.2.0",
    "flux": "^3.1.3",
    "identity-obj-proxy": "^3.0.0",
    "jquery": "^3.3.1",
    "jquery-ujs": "^1.2.2",
    "js-object-pretty-print": "^0.2.0",
    "jsc-android": "224109.x.x",
    "link": "^0.1.5",
    "lint": "^1.1.2",
    "mobx": "^5.0.3",
    "mobx-react": "^5.2.3",
    "native-base": "^2.7.2",
    "react": "16.3.1",
    "react-addons-test-utils": "^15.6.2",
    "react-clamp": "^1.0.5",
    "react-native": "0.55.4",
    "react-native-awesome-alerts": "^1.0.7",
    "react-native-camera": "^1.1.4",
    "react-native-circular-progress": "^0.2.0",
    "react-native-fcm": "^16.0.0",
    "react-native-firebase": "^4.2.0",
    "react-native-fs": "^2.11.15",
    "react-native-image-crop-picker": "^0.20.3",
    "react-native-image-gallery": "^2.1.5",
    "react-native-iphone-x-helper": "^1.0.3",
    "react-native-keyboard-aware-scroll-view": "^0.5.0",
    "react-native-keychain": "^3.0.0",
    "react-native-lightbox": "^0.7.0",
    "react-native-material-dropdown": "^0.11.1",
    "react-native-photo-upload": "^1.2.0",
    "react-native-router-flux": "4.0.0-beta.32",
    "react-native-select-multiple": "^1.2.0",
    "react-native-sentry": "^0.38.1",
    "react-native-simple-store": "^1.3.0",
    "react-native-snap-carousel": "^3.7.2",
    "react-native-spinkit": "^1.1.1",
    "react-native-svg": "^6.4.1",
    "react-native-svg-uri": "^1.2.3",
    "react-native-swiper-flatlist": "^1.0.7",
    "react-native-theming": "^1.0.16",
    "react-native-timer": "^1.3.3",
    "react-native-video": "^2.3.1",
    "react-native-view-pdf": "^0.3.1",
    "yarn": "^1.10.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-native": "4.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.1.0",
    "eslint-plugin-react": "^7.9.1",
    "husky": "^0.14.3",
    "lint-staged": "^7.1.3",
    "prettier": "^1.13.7",
    "prop-types": "^15.6.2",
    "react-dom": "^16.4.0",
    "react-test-renderer": "^16.4.1"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "^0.51.0",
    "mobx": "3.x || 4.x",
    "mobx-react": "4.x || 5.x"
  },
  "lint-staged": {
    "*.js": [
      "yarn pretty",
      "git add"
    ]
  }
}
